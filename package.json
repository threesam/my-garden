{
  "name": "digital-garden",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "echo 'No linter configured'",
    "analyze": "vite build --mode analyze",
    "perf": "vite build && vite preview --port 4173",
    "lighthouse": "npx lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "bundle-analyzer": "vite build && npx vite-bundle-analyzer"
  },
  "dependencies": {
    "svelte": "5.38.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@sveltejs/adapter-auto": "6.0.2",
    "@sveltejs/adapter-vercel": "5.8.2",
    "@sveltejs/kit": "2.27.3",
    "@tailwindcss/vite": "4.1.11",
    "svelte-check": "4.3.1",
    "tailwindcss": "4.1.11",
    "tslib": "2.8.1",
    "typescript": "~5.9.2",
    "vite": "7.1.1",
    "vite-bundle-analyzer": "1.2.0"
  }
}
